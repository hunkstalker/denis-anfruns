{
  "redirects": [
    {
      "source": "/es(/.*)?",
      "destination": "/es-es$1",
      "permanent": false
    },
    {
      "source": "/ca(/.*)?",
      "destination": "/ca-es$1",
      "permanent": false
    },
    {
      "source": "/",
      "has": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "(es-es).*"
        }
      ],
      "destination": "/es-es",
      "permanent": false
    },
    {
      "source": "/",
      "has": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "(ca-es).*"
        }
      ],
      "destination": "/ca-es",
      "permanent": false
    },
    {
      "source": "/",
      "has": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "(en).*"
        }
      ],
      "destination": "/en",
      "permanent": false
    }
  ]
}
